pragma solidity ^0.8.4;


//Import the standard implementation ERC721
import "@openzeppelin/contract/token/ERC721/ERC721.sol";

//contract 'firstNft' _IS_ an 'ERC721' contract
contract firstNft is ERC721 {

    constructor() ERC721 ("MyFirstNftasDEV","happy-dev"{
//mint 5 NFt for myself

_mint(msg.sender,1);
_mint(msg.sender,2);
_mint(msg.sender,3);
_mint(msg.sender,4);
_mint(msg.sender,5);
 }


}   
